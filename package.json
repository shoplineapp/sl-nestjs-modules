{
  "name": "sl-nest-module",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "release:build": "lerna run --parallel --scope \"@sl-nest-module/*\" build",
    "release:changelog": "ts-node ./scripts/createChangelog.ts",
    "release:publish": "lerna publish from-package --dist-tag latest --contents dist",
    "release:tag": "ts-node ./scripts/createTag.ts",
    "release:version": "lerna version --no-changelog --no-push --no-git-tag-version",
    "test": "lerna run --parallel --scope \"@sl-nest-module/*\" test"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.4",
    "lerna": "^4.0.0",
    "prettier": "^2.3.1",
    "yargs": "^17.2.1"
  }
}
